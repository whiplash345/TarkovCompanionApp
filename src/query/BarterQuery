const BarterQuery = `
  query GetBarters {
  barters{
    id
    trader{
      id
      name
      normalizedName
    }
    level
    taskUnlock{
      id
      name
      normalizedName
    }
    requiredItems{
      item{
        id
        name
        normalizedName
      }
      count
      quantity
    }
    rewardItems{
      item{
        id
        name
        normalizedName
      }
      count
      quantity
    }
    buyLimit
  }
}
`;